Step 2: Implement the Ray Class
python
复制代码
import numpy as np

class Ray:
    def __init__(self, position, direction):
        self.position = np.array(position)
        self.direction = np.array(direction) / np.linalg.norm(direction)
        self.history = [(self.position.copy(), self.direction.copy())]

    def append(self, pos, direc):
        self.history.append((np.array(pos), np.array(direc)))

    def update(self, new_position, new_direction):
        self.position = np.array(new_position)
        self.direction = np.array(new_direction) / np.linalg.norm(new_direction)
        self.append(self.position, self.direction)

    def vertices(self):
        return [pos for pos, _ in self.history]

    def directions(self):
        return [direc for _, direc in self.history]
Step 3: Implement the SphericalRefraction Class
python
复制代码
from optical_element import OpticalElement  # Assuming this is where your base class is

class SphericalRefraction(OpticalElement):
    def __init__(self, z_0, curvature, n_1, n_2, aperture):
        self._z_0 = z_0
        self._curvature = curvature
        self._n_1 = n_1
        self._n_2 = n_2
        self._aperture = aperture

    def z_0(self):
        return self._z_0

    def aperture(self):
        return self._aperture

    def curvature(self):
        return self._curvature

    def n_1(self):
        return self._n_1

    def n_2(self):
        return self._n_2

    def intercept(self, ray):
        # Implement the intercept calculation
        pass

    def propagate_ray(self, ray):
        intercept_point = self.intercept(ray)
        if intercept_point is None:
            return 

        if self._curvature != 0:
            O_vec = np.array([0, 0, self._z_0 + (1 / self._curvature)])
        else:
            return  # Planar surfaces not handled in this method

        OQ_vec = intercept_point - O_vec
        normal = OQ_vec / np.linalg.norm(OQ_vec)  # Normalize the normal vector

        new_direction = phys.refract(ray.direction, normal, self._n_1, self._n_2)
        if new_direction is None:
            return 

        ray.append(intercept_point, new_direction)
        return ray.vertices()
Step 4: Implement the Physics Module
python
复制代码
import numpy as np

def refract(direc, normal, n_1, n_2):
    # Implement Snell's law refraction
    cos_i = -np.dot(direc, normal)
    sin_t2 = (n_1 / n_2)**2 * (1 - cos_i**2)
    if sin_t2 > 1:
        return None  # Total internal reflection

    cos_t = np.sqrt(1 - sin_t2)
    return (n_1 / n_2) * direc + (cos_t - (n_1 / n_2) * cos_i) * normal
Step 5: Write Tests
python
复制代码
import numpy as np
from unittest.mock import MagicMock
import pytest
import elements  # Assuming elements.py contains SphericalRefraction
import rays     # Assuming rays.py contains Ray class

class TestSphericalRefraction:
    def test_pr_calls_append_once(self, monkeypatch):
        append_mock = MagicMock(return_value=None)
        with monkeypatch.context() as m:
            m.setattr(rays.Ray, "append", append_mock)
            if hasattr(elements, "Ray"):
                m.setattr(elements.Ray, "append", append_mock)

            sr = elements.SphericalRefraction(z_0=10, curvature=0.02, n_1=1., n_2=1.5, aperture=50.)
            sr.propagate_ray(rays.Ray([1., 2., 0.], [0., 0., 1.]))
        append_mock.assert_called_once()

        call_dict = {}
        for name, val in zip(signature(rays.Ray().append).parameters.keys(), append_mock.call_args.args):
            call_dict[name] = val
        call_dict.update(append_mock.call_args.kwargs)

        assert np.allclose(call_dict["pos"], [1., 2., 10.05002503])
        assert np.allclose(call_dict["direc"], [-0.00667112, -0.01334223, 0.99988873])
